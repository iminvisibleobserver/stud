#include <iostream>
using namespace std;

int climbStairs(int n) {
    if (n <= 1) {
        return 1;
    }
    
    int a = 1, b = 1;
    for (int i = 2; i <= n; ++i) {
        int temp = a + b;
        a = b;
        b = temp;
    }
    return b;
}

int main() {
    // Приклади
    int n1 = 2;
    int n2 = 3;

    cout << "Number of ways to climb " << n1 << " steps: " << climbStairs(n1) << endl;
    cout << "Number of ways to climb " << n2 << " steps: " << climbStairs(n2) << endl;

    return 0;
}
