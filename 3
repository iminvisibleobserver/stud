#include <iostream>
using namespace std;

// Функція для обчислення числа Фібоначчі
int fibonacci(int n) {
    if (n <= 1) {
        return n;
    }
    
    int a = 0, b = 1;
    for (int i = 2; i <= n; ++i) {
        int temp = a + b;
        a = b;
        b = temp;
    }
    return b;
}

int main() {
    
    int n1 = 2;
    int n2 = 3;
    int n3 = 4;

    cout << "F(" << n1 << ") = " << fibonacci(n1) << endl;
    cout << "F(" << n2 << ") = " << fibonacci(n2) << endl;
    cout << "F(" << n3 << ") = " << fibonacci(n3) << endl;

    return 0;
}
